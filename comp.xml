<?xml version="1.0" encoding="UTF-8"?>
<query xmlns:yahoo="http://query.yahooapis.com/v1/schema/table.xsd">
    <diagnostics>
        <publiclyCallable>true</publiclyCallable>
        <user-time>24</user-time>
        <service-time>2</service-time>
        <build-version>9597</build-version>
    </diagnostics> 
  <bindings>
    <select itemPath="root.crimes" produces="XML">
      <urls>
        <url>http://github.com/gh269/cornellDataTable/raw/master/test.xml</url>
      </urls>
    </select>
  </bindings>
    <results>
            <meta>
                <author>Josh Gordineer</author>
                <description>Yelp Review Search API</description>
                <documentationURL>http://www.yelp.com/developers/documentation/search_api</documentationURL>
                <sampleQuery>select * from yelp.review.search where term='pizza' and location='sunnyvale, ca' and ywsid='6L0Lc-yn1OKMkCKeXLD4lg'</sampleQuery>
            </meta>
            <request>
                <select>
                    <key name="incident" required="true" type="xs:string"/>
                </select>

            </request>
    </results>
</query>
 